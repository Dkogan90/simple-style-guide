<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta charset="UTF-8"/>
        <title>Styleguide</title>

        <!--Vendor styles-->
        {{#each vendorStyle}}
            <link rel="stylesheet" type="text/css" href="{{this}}"/>
        {{/each}}
        <!--Vendor styles-->

        <!--Costume styles-->
        {{#each customStyle}}
            <link rel="stylesheet" type="text/css" href="{{this}}"/>
        {{/each}}
        <!--Costume styles-->

        <!--Vendor scripts!-->
        {{#each vendorScript}}
            <script type="text/javascript" src="{{this}}"></script>
        {{/each}}
        <!--Vendor scripts!-->

        <!--Costume script-->
        {{#each customScript}}
            <script type="text/javascript" src="{{this}}"></script>
        {{/each}}
        <!--Costume script-->
    </head>
    <body>
        <div id="wrapper">
            {{> sidebar}}
            <!-- Page content -->
            <div id="page-content-wrapper">
                <div class="container col-xs-12">
                    {{> header }}
                    {{> content}}
                </div>
            </div>
        </div>
    </body>
</html>
